{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wpwing/toc",
	"version": "1.0.6",
	"title": "Table Of Contents",
	"category": "widgets",
	"keywords": [ "TOC", "Table of Contents", "WPWing" ],
	"icon": "list-view",
	"description": "Adds a Table of Contents.",
	"attributes": {
		"max_level": {
			"type": "integer",
			"default": 6
		},
		"no_title": {
			"type": "boolean",
			"default": false
		},
		"use_ol": {
			"type": "boolean",
			"default": false
		},
		"remove_indent": {
			"type": "boolean",
			"default": false
		},
		"use_absolute_urls": {
			"type": "boolean",
			"default": false
		},
		"add_smooth": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"align": [ "wide" , "full"]
	},
	"textdomain": "wpwing-table-of-contents-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
